<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.itwill.billycar.mapper.EventMapper">
	<insert id="updateEvent">
		INSERT INTO event
		VALUES (
			null
			, #{event_title}
			, #{event_content}
			, #{event_image}
		)
	</insert>

</mapper>